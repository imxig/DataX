{
    "job": {
        "setting": {
            "speed": {
                 "channel":1
            }
        },
        "content": [
            {
                "reader": {
                    "name": "clickhousereader",
                    "parameter": {
                        "username": "default",
                        "password": "xxxxxx",
                        "connection": [
                            {
                                "querySql": [
                                    "SELECT strategyId,statisticTime,period,srcIp,destIp,destPort,dbName,tableName,fieldName,sensitiveLevel,sensitiveType,visitCount,source,createTime FROM dsc.db_ip_visit_statistic where createTime >= date_sub(day, 90, toDate(now()))"
                                ],
                                "jdbcUrl": [
                                    "jdbc:clickhouse://xxx.xxx.xxx.xxx:8123/dsc"
                                ]
                            }
                        ],
						"fetchSize": 1000,
						"queryTimeOut": 5000
                    }
                },
                "writer": {
                    "name":"elasticsearchwriter",
                    "parameter":{
                        "endpoint":"http://xxx.xxx.xxx.xxx:19200",
                        "index":"xxx-ds-db_ip_visit_statistic",
                        "type":"_doc",
                        "dynamic": true,
                        "batchSize":10000,
                        "trySize":2,
                        "splitter":",",
                        "column":[{"name":"strategyId","type":"keyword"},
                            {"name":"statisticTime","type":"long"},
                            {"name":"period","type":"integer"},
                            {"name":"srcIp","type":"keyword"},
                            {"name":"destIp","type":"keyword"},
                            {"name":"destPort","type":"integer"},
                            {"name":"dbName","type":"keyword"},
                            {"name":"tableName","type":"keyword"},
                            {"name":"fieldName","type":"keyword"},
                            {"name":"sensitiveLevel","type":"keyword"},
                            {"name":"sensitiveType","type":"keyword"},
                            {"name":"visitCount","type":"integer"},
                            {"name":"dsSource","type":"keyword"},
                            {"name":"createTime","type":"date","format":"yyyy-MM-dd HH:mm:ss||strict_date_optional_time||epoch_millis"}]
                    }
                }
            }
        ]
    }
}